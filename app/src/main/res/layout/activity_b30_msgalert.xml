                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/size_5"
                        android:background="@mipmap/h9_mine_notice"
                        android:visibility="gone" />


