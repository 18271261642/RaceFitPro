                    android:layout_height="1dp"
                    android:background="@color/linear_border" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/square_white_btn_change"
                    android:padding="5dp">

