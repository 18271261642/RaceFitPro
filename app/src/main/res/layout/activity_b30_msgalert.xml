            android:layout_height="match_parent"
            android:orientation="vertical">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="#ee3a8c"
                android:gravity="center"
                android:padding="5dp"
                android:text="@string/notif_messge"
                android:textColor="@color/white"
                android:textSize="18sp" />


            <LinearLayout
                android:id="@+id/msgOpenNitBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/b18i_selected_bg"
                android:gravity="center"
                android:orientation="horizontal"
                android:padding="@dimen/size_10">

                <ImageView
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/size_5"
                    android:gravity="center_vertical"
                    android:padding="@dimen/size_5"
                    android:src="@mipmap/ic_msg_open_noti" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/size_15"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:padding="@dimen/size_5"
                    android:text="@string/open_notif"
                    android:textColor="#333333"
                    android:textSize="@dimen/text_size_17" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_marginRight="@dimen/size_15"
                    android:src="@mipmap/lanage" />

            </LinearLayout>


            <LinearLayout
