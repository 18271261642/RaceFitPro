            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center"
                android:orientation="horizontal"
                android:padding="@dimen/size_10">

                <ImageView
                    android:layout_width="@dimen/size_35"
                    android:layout_height="@dimen/size_35"
                    android:layout_marginLeft="@dimen/size_5"
                    android:gravity="center_vertical"
                    android:padding="@dimen/size_5"
                    android:src="@mipmap/image_qq_notif" />

                <TextView

                    style="@style/B30ToggTxtStyle"
                    android:text="QQ" />

                <ToggleButton
                    android:id="@+id/b30QQTogg"
                    style="@style/B30MsgToggleStyle" />

            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/size_20"
                android:background="#dddddd" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center"
                android:orientation="horizontal"
                android:padding="@dimen/size_10">

                <ImageView
                    android:layout_width="@dimen/size_35"
                    android:layout_height="@dimen/size_35"
                    android:layout_marginLeft="@dimen/size_5"
                    android:gravity="center_vertical"
                    android:padding="@dimen/size_5"
                    android:src="@mipmap/image_msg_notif" />

                <TextView

                    style="@style/B30ToggTxtStyle"
                    android:text="@string/string_new_messege" />

                <ToggleButton
                    android:id="@+id/b30MessageTogg"
                    style="@style/B30MsgToggleStyle" />

            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/size_20"
                android:background="#dddddd" />

            <LinearLayout
